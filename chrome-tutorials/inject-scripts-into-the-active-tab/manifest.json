{
	"name": "Google Results",
	"description": "View Google search results in a simplified format.",
	"version": "1.0",
	"manifest_version": 3,
	"permissions": ["scripting", "activeTab"],
	"commands": {
		"_execute_action": {
			"suggested_key": {
				"default": "Ctrl+P",
				"mac": "Alt+P"
			}
		}
	},
	"background": {
		"service_worker": "background.js"
	},
	"icons": {
		"16": "images/icon-16.png",
		"32": "images/icon-32.png",
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	},
	"action": {
		"default_icon": {
			"16": "images/icon-16.png",
			"32": "images/icon-32.png",
			"48": "images/icon-48.png",
			"128": "images/icon-128.png"
		}
	}
}
