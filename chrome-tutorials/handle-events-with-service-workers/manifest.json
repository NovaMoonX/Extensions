{
	"name": "MDD Omnibox",
	"description": "Uses the omnibox to quickly search MoonDreamDev resources",
	"version": "1.0",
	"manifest_version": 3,
	"icons": {
		"16": "images/icon-16.png",
		"128": "images/icon-128.png"
	},
	"background": {
		"service_worker": "service-worker.js",
		"type": "module"
	},
	"permissions": ["storage"],
	"minimum_chrome_version": "102",
	"omnibox": {
		"keyword": "mdd"
	}
}
